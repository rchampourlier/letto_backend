// This file provides a simple entrypoint to test the
// JS execution environment. You can use it by running:
//
//     $ go run main.go
//
// from the `exec` directory.

package main

import (
	"os"

	"github.com/rchampourlier/letto_go/exec/js"
)

func main() {
	dir, _ := os.Getwd()
	js.Run("node:4",
		[]string{"node", "./js/test.js"},
		map[string]struct{}{"/usr/src/app": {}},
		"/usr/src/app",
		[]string{dir + ":/usr/src/app"},
	)
}
